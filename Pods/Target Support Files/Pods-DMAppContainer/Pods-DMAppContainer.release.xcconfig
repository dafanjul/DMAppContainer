FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/DMCoreFramework.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/DMFlightsFramework.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/DMHotelFramework.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "DMFlightsFramework" -framework "DMHotelFramework"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-DMAppContainer
PODS_ROOT = ${SRCROOT}/Pods